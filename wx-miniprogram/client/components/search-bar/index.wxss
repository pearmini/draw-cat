.container {
  width: calc(100% - 80rpx);
  height: 80rpx;
  border-radius: 40rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 40rpx;
}

.icon {
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  transition: all 0.1s;
  height: 100%;
}

.input {
  width: 90%;
}
